{
  "type": "object",
  "comment": "testdata",
  "properties": {
    "isMyView": {
      "type": "identifier",
      "selector": ".subtle",
      "control": "exists",
      "selectContains": "Assigned to Me"
    },
    "viewissueslink": {
      "type": "action",
      "selector": "a",
      "control": "action.link",
      "selectContains": "View Issues"
    },
    "list": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "ID": {
            "type": "string"
          },
          "stauts": {
            "type": "string"
          },
          "summary": {
            "type": "string"
          }
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "isMyView"
      ],
      "action": "viewissueslink"
    }
  ]
}
