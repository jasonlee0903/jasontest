{
  "type": "object",
  "comment": "testdata",
  "properties": {
    "isMyView": {
      "type": "identifier",
      "selector": "a[href*='my_view_page.php']",
      "control": "exists"
    },
    "viewissueslink": {
      "type": "action",
      "selector": "a",
      "control": "action.link",
      "selectContains": "View Issues"
    }
  },
  "connect": [
    {
      "match": [
        "isMyView"
      ],
      "action": "viewissueslink"
    }
  ]
}
