{
  "type": "object",
  "properties": {
    "list": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "ID": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 3,
            "control": "property.innerText"
          },
          "stauts": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 8,
            "control": "property.innerText"
          },
          "summary": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selectIndex": 10,
            "selector": "td",
            "control": "property.innerText"
          }
        }
      },
      "update": {
        "type": "none"
      },
      "selectorAll": "#buglist tbody tr[bgcolor]"
    },
    "isViewissues": {
      "type": "identifier",
      "selector": ".floatleft",
      "control": "exists",
      "selectContains": "Viewing Issues"
    }
  },
  "connect": [
    {
      "match": [
        "isViewissues"
      ],
      "screens": [
        "viewissues"
      ]
    }
  ]
}
